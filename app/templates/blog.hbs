
{{!< default}}

{{head-er userData=userData}}

{{!-- The main content area --}}
<div id="site-main" class="site-main outer margin-top-100">
    <div class="inner">

        <div class="post-feed">
            {{#each (sort-by 'date:desc' model) as |post index|}}
              {{#if post.status}}
                {{!-- The tag below includes the markup for each post - partials/post-card.hbs --}}
                <PostCard @index={{index}} @post={{post}}/>
              {{/if}}
            {{/each}}
        </div>

    </div>
</div>